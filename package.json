{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "server": "nodemon --esm src/server/main.ts -w src/server",
    "start": "NODE_ENV=production ts-node src/server/main.ts",
    "build": "vite build",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --outDir dist/server --ssr src/entry-server.js"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "chalk": "^4.1.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-media-recorder": "^1.6.6",
    "react-router-dom": "^6.11.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "vite-express": "^0.9.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/chalk": "^2.2.0",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^18.11.18",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^3.0.1",
    "autoprefixer": "^10.4.14",
    "nodemon": "^2.0.20",
    "postcss": "^8.4.23",
    "tailwindcss": "^3.3.2",
    "vite": "^4.0.4"
  }
}
